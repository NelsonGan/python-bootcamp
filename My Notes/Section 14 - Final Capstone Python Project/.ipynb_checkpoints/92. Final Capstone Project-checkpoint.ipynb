   "source": [
    "#Fibonacci Sequence\n",
    "\n",
    "def fibo_sequence(n):\n",
    "    \n",
    "    a = 0\n",
    "    b = 1\n",
    "    ret = 0\n",
    "    \n",
    "    for i in range(n):\n",
    "        ret = a\n",
    "        yield ret\n",
    "        a = b\n",
    "        b = ret + b"
   ]
  },
